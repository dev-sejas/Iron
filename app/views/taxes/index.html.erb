<p id="notice"><%= notice %></p>

<h1>Taxes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type tax use</th>
      <th>Tax adjustement</th>
      <th>Amount type</th>
      <th>Active</th>
      <th>Company</th>
      <th>Amount</th>
      <th>Account</th>
      <th>Refund account</th>
      <th>Description</th>
      <th>Price include</th>
      <th>Include base amount</th>
      <th>Analytic</th>
      <th>Tax exigible</th>
      <th>Cash basis account</th>
      <th>Create uid</th>
      <th>Write uid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taxes.each do |tax| %>
      <tr>
        <td><%= tax.name %></td>
        <td><%= tax.type_tax_use %></td>
        <td><%= tax.tax_adjustement %></td>
        <td><%= tax.amount_type %></td>
        <td><%= tax.active %></td>
        <td><%= tax.company_id %></td>
        <td><%= tax.amount %></td>
        <td><%= tax.account_id %></td>
        <td><%= tax.refund_account_id %></td>
        <td><%= tax.description %></td>
        <td><%= tax.price_include %></td>
        <td><%= tax.include_base_amount %></td>
        <td><%= tax.analytic %></td>
        <td><%= tax.tax_exigible %></td>
        <td><%= tax.cash_basis_account %></td>
        <td><%= tax.create_uid %></td>
        <td><%= tax.write_uid %></td>
        <td><%= link_to 'Show', tax %></td>
        <td><%= link_to 'Edit', edit_tax_path(tax) %></td>
        <td><%= link_to 'Destroy', tax, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tax', new_tax_path %>

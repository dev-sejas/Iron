<p id="notice"><%= notice %></p>

<h1>Box Details</h1>

<table>
  <thead>
    <tr>
      <th>Box</th>
      <th>Payment</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @box_details.each do |box_detail| %>
      <tr>
        <td><%= box_detail.box_id %></td>
        <td><%= box_detail.payment_id %></td>
        <td><%= box_detail.amount %></td>
        <td><%= link_to 'Show', box_detail %></td>
        <td><%= link_to 'Edit', edit_box_detail_path(box_detail) %></td>
        <td><%= link_to 'Destroy', box_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Box Detail', new_box_detail_path %>

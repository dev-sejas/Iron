<table class="table">
  <thead>
    <tr>
 <!-- aqui paginaciÃ³n -->
    </tr>
    <tr>
      <th>Nombre </th>
      <th>Description</th>
      <th>Moneda</th>
      <th>Activo</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
   <% @boxes.each do |box| %>
      <tr>
      <td><%= box.name %></td>
       <td><%= box.description %></td>
        <td><%= box.currency_id %></td>
        <td><%= box.active %></td>
        <td><%= link_to 'Show', box %></td>
        <td><%= link_to 'Edit', edit_box_path(box) %></td>
        <td><%= link_to 'Destroy', box, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>




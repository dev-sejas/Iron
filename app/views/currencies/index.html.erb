<div class="panel panel-purchase">
  <div class="panel-head">Moneda</div>
  <div class="panel-wrapper collapse in">
    <div class="panel-body table-responsive ">
       <%= render 'table', currency: @currency %>

         <div class="col-md-5 col-md-offset-5 col-sm-5 col-sm-offset-5">
      <p>
          <%= button_to new_currency_path, {method: :get, class: "btn btn-success"} do %>
          <span class="glyphicon glyphicon-plus" aria-hidden="true"> </span> Nueva Moneda
          <% end %>
      </p>
      </div>
    </div>
  </div>
</div>




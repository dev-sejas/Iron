<p id="notice"><%= notice %></p>

<h1>Checks</h1>

<table>
  <thead>
    <tr>
      <th>Bank</th>
      <th>Number</th>
      <th>Date giro</th>
      <th>Date payment</th>
      <th>Titular</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @checks.each do |check| %>
      <tr>
        <td><%= check.bank_id %></td>
        <td><%= check.number %></td>
        <td><%= check.date_giro %></td>
        <td><%= check.date_payment %></td>
        <td><%= check.titular %></td>
        <td><%= check.amount %></td>
        <td><%= link_to 'Show', check %></td>
        <td><%= link_to 'Edit', edit_check_path(check) %></td>
        <td><%= link_to 'Destroy', check, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Check', new_check_path %>

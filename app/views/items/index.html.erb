

<div class="panel panel-inf">
  <div class="panel-head">Articulos(Productos)</div>
    <div class="panel-wrapper collapse in" aria-expanded="true">
      <div class="panel-body table-responsive ">
        <%= render partial: 'layouts/partials/search_form', locals: { search_entity: 'items', word: "Articulo", url: new_item_path, view_list:'items_view_list',view_kamback: 'items_view_kamback', ban: false} %>
            <% if @list %>
                <%= render partial: 'results_search' %>
          <% else %> 
                <%= render partial: 'results_search_camback' %>
           <% end %>

          <div class="col-md-5 col-md-offset-5 col-sm-5 col-sm-offset-5">
            <p>
              <%= button_to new_item_path, {method: :get, class: "btn btn-success"} do %>
                <span class="glyphicon glyphicon-plus" aria-hidden="true"> </span> Nuevo articulo
              <% end %>
            </p>
          </div>
    </div>  
  </div>
  </div>


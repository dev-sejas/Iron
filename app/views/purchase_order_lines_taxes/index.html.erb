<p id="notice"><%= notice %></p>

<h1>Purchase Order Lines Taxes</h1>

<table>
  <thead>
    <tr>
      <th>Purchse order line</th>
      <th>Tax</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @purchase_order_lines_taxes.each do |purchase_order_lines_tax| %>
      <tr>
        <td><%= purchase_order_lines_tax.purchase_order_line_id %></td>
        <td><%= purchase_order_lines_tax.tax_id %></td>
        <td><%= link_to 'Show', purchase_order_lines_tax %></td>
        <td><%= link_to 'Edit', edit_purchase_order_lines_tax_path(purchase_order_lines_tax) %></td>
        <td><%= link_to 'Destroy', purchase_order_lines_tax, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

